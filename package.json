{
  "name": "jules-extension",
  "displayName": "Jules Extension",
  "description": "Control Google Jules AI coding agent directly from VS Code",
  "version": "1.0.9",
  "publisher": "HirokiMukai",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/is0692vs/jules-extension.git"
  },
  "galleryBanner": {
    "color": "#7B3FF2",
    "theme": "dark"
  },
  "keywords": [
    "Jules",
    "coding-agent",
    "LLM",
    "ai",
    "vscode-extension",
    "productivity",
    "automation",
    "google",
    "development-tools",
    "artificial-intelligence",
    "coding-assistant",
    "code-agent",
    "ai-agent",
    "gemini"
  ],
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Jules Extension",
      "properties": {
        "jules-extension.apiKey": {
          "type": "string",
          "default": "",
          "description": "API Key for Jules API"
        },
        "jules-extension.autoRefresh.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable automatic refresh of sessions list"
        },
        "jules-extension.autoRefresh.interval": {
          "type": "number",
          "default": 30,
          "minimum": 10,
          "description": "Auto-refresh interval in seconds (minimum: 10)"
        },
        "jules-extension.customPrompt": {
          "type": "string",
          "default": "",
          "description": "A custom prompt to automatically prepend to every message sent to Jules. This acts as a persistent instruction.",
          "markdownDescription": "Enter a custom prompt that will be automatically added to the beginning of every message you send to Jules. This is useful for providing persistent instructions or context.\n\n**Example:**\n`Always reply in Japanese.`"
        },
        "jules-extension.hideClosedPRSessions": {
          "type": "boolean",
          "default": true,
          "description": "Automatically hide sessions with closed or merged pull requests from the session list"
        },
        "jules-extension.githubToken": {
          "type": "string",
          "default": "",
          "deprecationMessage": "This setting is deprecated. Use the 'Jules: Set GitHub Token' command instead for secure storage.",
          "description": "[DEPRECATED] Use command 'Jules: Set GitHub Token' instead. GitHub Personal Access Token for checking PR status (optional, uses public API if not set)",
          "markdownDescription": "⚠️ **DEPRECATED:** Please use the command `Jules: Set GitHub Token` instead to store your token securely.\n\nThis setting is only for display purposes. Tokens should be stored using VS Code's secure secrets storage via the command palette."
        },
        "jules.defaultBranch": {
          "type": "string",
          "enum": [
            "current",
            "default",
            "main"
          ],
          "enumDescriptions": [
            "Use current Git branch as default",
            "Use repository's default branch",
            "Use 'main' branch (fallback to default if not exists)"
          ],
          "default": "current",
          "description": "Which branch to select by default when creating a session"
        }
      }
    },
    "commands": [
      {
        "command": "jules-extension.setApiKey",
        "title": "Set Jules API Key",
        "icon": "$(key)"
      },
      {
        "command": "jules-extension.verifyApiKey",
        "title": "Verify Jules API Key"
      },
      {
        "command": "jules-extension.listSources",
        "title": "List Jules Sources",
        "icon": "$(repo)"
      },
      {
        "command": "jules-extension.createSession",
        "title": "Create Jules Session",
        "icon": "$(add)"
      },
      {
        "command": "jules-extension.refreshSessions",
        "title": "Refresh Jules Sessions",
        "icon": "$(refresh)"
      },
      {
        "command": "jules-extension.showActivities",
        "title": "Show Jules Activities"
      },
      {
        "command": "jules-extension.refreshActivities",
        "title": "Refresh Jules Activities",
        "icon": "$(refresh)"
      },
      {
        "command": "jules-extension.sendMessage",
        "title": "Send Message to Jules Session"
      },
      {
        "command": "jules-extension.approvePlan",
        "title": "Approve Jules Plan",
        "category": "Jules",
        "icon": "$(check)"
      },
      {
        "command": "jules-extension.openSettings",
        "title": "Open Jules Settings",
        "category": "Jules",
        "icon": "$(settings-gear)"
      },
      {
        "command": "jules-extension.deleteSession",
        "title": "Delete Session from Local Cache",
        "category": "Jules",
        "icon": "$(trash)"
      },
      {
        "command": "jules-extension.setGithubToken",
        "title": "Set GitHub Token (for PR Status)",
        "category": "Jules",
        "icon": "$(github)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "julesSessionsView",
          "name": "Jules Sessions",
          "icon": "$(robot)",
          "contextualTitle": "Jules Sessions"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "jules-extension.createSession",
          "when": "view == julesSessionsView",
          "group": "navigation@1"
        },
        {
          "command": "jules-extension.listSources",
          "when": "view == julesSessionsView",
          "group": "navigation@2"
        },
        {
          "command": "jules-extension.refreshSessions",
          "when": "view == julesSessionsView",
          "group": "navigation@3"
        },
        {
          "command": "jules-extension.openSettings",
          "when": "view == julesSessionsView",
          "group": "navigation@4"
        },
        {
          "command": "jules-extension.setApiKey",
          "when": "view == julesSessionsView",
          "group": "navigation@5"
        }
      ],
      "view/item/context": [
        {
          "command": "jules-extension.sendMessage",
          "when": "view == julesSessionsView && viewItem == jules-session",
          "group": "inline"
        },
        {
          "command": "jules-extension.deleteSession",
          "when": "view == julesSessionsView && viewItem == jules-session",
          "group": "management"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/sinon": "^17.0.0",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "npm-run-all": "^4.1.5",
    "sinon": "^17.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "is-glob": "^4.0.3"
  }
}
